@model vmParameters
<label class="label" for="param-@Model.Id">@Model.ParamterTitle</label>
<input type="hidden" name="model[@Model.ParameterOrder].RegexValidator" value="@Model.RegexValidator" />
<input type="hidden" name="model[@Model.ParameterOrder].Id" value="@Model.Id" />
<input id="param-@Model.Id" type="number" name="model[@Model.ParameterOrder].Value" class="form-control" @(Model.IsMandotory ? "required" : "") @(String.IsNullOrEmpty(Model.RegexValidator) ? "" : "pattern=" + Model.RegexValidator) />